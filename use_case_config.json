{
  "import": {
    "default_missing_value": "Not specified",
    "fields": {
      "title": {
        "column": "Use Case Name",
        "required": true,
        "default": "Untitled use case"
      },
      "industry": {
        "column": "Business Sector",
        "default": "Not specified",
        "label": "Business sector"
      },
      "summary": {
        "column": "Description",
        "default": "Not specified",
        "label": "Description"
      },
      "synergy": {
        "column": "Synergy",
        "default": "Not specified",
        "label": "Synergy"
      },
      "problem": {
        "column": "ProblemStatement",
        "default": "Not specified",
        "label": "Problem statement"
      },
      "solution": {
        "column": "SolutionDescription",
        "default": "Not specified",
        "label": "Solution description"
      },
      "impact": {
        "column": "Business Value Estimate (Numerical)",
        "default": "0",
        "label": "Business value estimate"
      },
      "data_source": {
        "column": "Source Stage",
        "default": "Not specified",
        "label": "Source stage"
      },
      "status_color": {
        "column": "New Type of Business Benefit",
        "default": "Not specified",
        "label": "Business benefit type"
      },
      "tags": {
        "column": "New Domain Mapping for G Level",
        "default": "",
        "label": "Domain mapping"
      }
    }
  },
  "access_control": {
    "roles": {
      "admin": {
        "visible_fields": [
          "*"
        ]
      },
      "reader": {
        "visible_fields": [
          "title",
          "industry",
          "synergy",
          "summary",
          "problem",
          "solution",
          "data_source",
          "status_color"
        ]
      }
    },
    "ownership": {
      "full_access": true,
      "additional_fields": []
    }
  },
  "visualizations": {
    "color_palette": [
  "#1988c7",  
  "#a24a83", 
  "#118465",  
  "#5c1b46",  
  "#102c45",  
  "#64807e",  
  "#9b9a91",  
  "#b8b7aa",  
  "#d8d7cc",  
  "#f4f2eb"  
],
    "filters": [
      {
        "id": "business-sector",
        "field": "industry",
        "label": "Business sector",
        "type": "select",
        "allowed_roles": ["admin", "reader"],
        "empty_label": "All sectors",
        "missing_label": "Not specified",
        "help_text": "Select one or more sectors."
      },
      {
        "id": "benefit-type",
        "field": "status_color",
        "label": "Business benefit type",
        "type": "select",
        "allowed_roles": ["admin", "reader"],
        "empty_label": "All benefit types",
        "missing_label": "Not specified",
        "help_text": "Select one or more benefit types."
      },
      {
        "id": "synergy",
        "field": "synergy",
        "label": "Synergy",
        "type": "select",
        "allowed_roles": ["admin", "reader"],
        "empty_label": "All synergies",
        "missing_label": "Not specified",
        "help_text": "Filter by synergy descriptions."
      },
      {
        "id": "domain",
        "field": "tags",
        "label": "Domain",
        "type": "select",
        "allowed_roles": ["admin", "reader"],
        "empty_label": "All domains",
        "missing_label": "Not specified",
        "value_separator": ",",
        "help_text": "Filter by tagged domains."
      },
      {
        "id": "rollout-stage",
        "field": "data_source",
        "label": "Roll out stage",
        "type": "select",
        "allowed_roles": ["admin", "reader"],
        "empty_label": "All stages",
        "missing_label": "Not specified",
        "help_text": "Filter by roll out stage."
      },
      {
        "id": "business-value",
        "field": "impact",
        "label": "Business value",
        "type": "range",
        "allowed_roles": ["admin"],
        "placeholder_min": "Min value",
        "placeholder_max": "Max value",
        "unit": "Estimated value"
      }
    ],
    "graphs": [
      {
        "id": "business-value-by-rollout-stage",
        "title": "Business value by roll out stage",
        "description": "Sum of estimated business value grouped by roll out stage.",
        "type": "donut",
        "group_by": "data_source",
        "metric": {
          "operation": "sum",
          "field": "impact",
          "label": "Business value"
        },
        "allowed_roles": [
          "admin",
          "reader"
        ],
        "missing_label": "Not specified"
      },
      {
        "id": "business-value-by-sector",
        "title": "Business value by business sector",
        "description": "Sum of estimated business value grouped by business sector.",
        "type": "donut",
        "group_by": "industry",
        "metric": {
          "operation": "sum",
          "field": "impact",
          "label": "Business value"
        },
        "allowed_roles": [
          "admin",
          "reader"
        ],
        "missing_label": "Not specified"
      },
      {
        "id": "business-value-by-domain",
        "title": "Business value by domain",
        "description": "Sum of estimated business value across the tagged domains.",
        "type": "donut",
        "group_by": "tags",
        "group_value_separator": ",",
        "metric": {
          "operation": "sum",
          "field": "impact",
          "label": "Business value"
        },
        "allowed_roles": [
          "admin",
          "reader"
        ],
        "missing_label": "Not specified"
      },
      {
        "id": "business-value-by-synergy",
        "title": "Business value by synergy",
        "description": "Sum of estimated business value grouped by synergy classification.",
        "type": "donut",
        "group_by": "synergy",
        "metric": {
          "operation": "sum",
          "field": "impact",
          "label": "Business value"
        },
        "allowed_roles": [
          "admin",
          "reader"
        ],
        "missing_label": "Not specified"
      },
      {
        "id": "business-value-by-benefit-type",
        "title": "Business value by benefit type",
        "description": "Sum of estimated business value for each business benefit classification.",
        "type": "bar",
        "group_by": "status_color",
        "metric": {
          "operation": "sum",
          "field": "impact",
          "label": "Business value"
        },
        "allowed_roles": [
          "admin",
          "reader"
        ],
        "missing_label": "Not specified",
        "chart_options": {
          "scales": {
            "y": {
              "beginAtZero": true
            }
          }
        }
      }
    ]
  }
}
